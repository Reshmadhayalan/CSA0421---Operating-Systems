int main()
{
    int n;
    printf("Enter no .of process:");
    scanf("%d" ,& n);

    int bt[n], wt[n], tot( n);

    printf("Enter Burst Time : \n");
for (i = 0; i < n; i++)
{
    Scand("% d" & bt[i]);
    wt[0] = 0;

    for (i = 1; i < n; i++)
        wt[i] = wt[i - 1] + bt[i - 1];

    printf("Complete BT & WT LIST \n");
}
